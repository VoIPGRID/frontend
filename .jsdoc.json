{
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc"]
    },
    "source": {
        "includePattern": ".js$"
    },
    "plugins": [
        "plugins/markdown"
    ],
    "templates" : {
        "logo": {
            "url": "https://garage11.io/public/img/logo.png",
            "width": "72px",
            "height": "60px"
        },
        "name": "VoIPGRID frontend",
        "footerText": "(c) Devhouse Spindle",
        "useCollapsibles": true,
        "tabNames": {
            "api": "API",
            "tutorials": "Manuals"
        }
    },
    "opts": {
        "destination": "./docs/build",
        "encoding": "utf8",
        "private": true,
        "recurse": true,
        "template": "node_modules/jsdoc-rtd",
        "tutorials": "./docs/manuals"
    },
    "package": "./package.json"
}
